{
  "version": "0.1.0",
  "description": "An Demo snap for swapping assets using Garden API.",
  "proposedName": "Garden Swap",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "+ivOf1U6a6ezD+nT9jx7T6x0+BLkQ884epmsWJ+25j8=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "src/assets/garden_logo.svg",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    },
    "snap_notify": {},
    "endowment:network-access": {},
    "snap_manageState": {},
    "snap_getBip32Entropy": [
      {
        "path": ["m", "44'", "1'", "0'", "0", "0"],
        "curve": "secp256k1"
      }
    ],
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "*/10 * * * * *",
          "request": {
            "method": "checkOrderStatus"
          }
        }
      ]
    }
  },
  "platformVersion": "6.14.0",
  "manifestVersion": "0.1"
}
